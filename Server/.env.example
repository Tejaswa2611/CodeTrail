# Environment Variables
NODE_ENV=development
PORT=3001

# JWT Secrets
JWT_SECRET=your-super-secret-jwt-key-here-make-this-very-long-and-complex
JWT_REFRESH_SECRET=your-super-secret-refresh-key-here-make-this-different-and-long
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Security
BCRYPT_ROUNDS=12
